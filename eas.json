{
  "cli": {
    "version": ">= 3.5.0"
  },
  "build": {
    "development": {
      "releaseChannel": "development",
      "developmentClient": true,
      "distribution": "store",
      "ios": {
        "resourceClass": "m1-medium",
        "credentialsSource": "local" 
      }
    },
    "preview": {
      "releaseChannel": "preview",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "releaseChannel": "prod-v1",
      "ios": {
        "resourceClass": "m1-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
